<struts>
	<package name="dang-order" extends="dang-default" namespace="/order">
		<action name="orderAction" class="com.tarena.web.action.OrderAction">
			<result name="address_form" type="dispatcher">
				/WEB-INF/jsp/order/address_form.jsp
