<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.tarena.entity.Book" table="d_book">
		<id column="id" name="id" type="integer">
			<!-- generator用来指明主键的生成方式 -->
			<generator class="native" />
		</id>
		<property column="author" name="author" type="string" />
		<property column="publishing" name="publishing" type="string" />
		<property column="publish_time" name="publishTime" type="long" />
		<property column="word_number" name="wordNumber" type="string" />
		<property column="which_edtion" name="whichEdtion" type="string" />
		<property column="total_page" name="totalPage" type="string" />
		<property column="print_time" name="printTime" type="integer" />
		<property column="print_number" name="printNumber" type="string" />
		<property column="isbn" name="isbn" type="string" />
		<property column="author_summary" name="authorSummary" type="string" />
		<property column="catalogue" name="catalogue" type="string" />
	</class>
</hibernate-mapping>
