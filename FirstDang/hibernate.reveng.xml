<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="d_book" catalog="dangdang"
		class="org.colin.dang.pojo.Book">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
		<column name="publish_time"></column>
		<column name="word_number"></column>
		<column name="which_edtion"></column>
		<column name="total_page"></column>
		<column name="print_time"></column>
		<column name="print_number"></column>
		<column name="author_summary"></column>
	</table>
	<table name="d_category_product" catalog="dangdang"
		class="org.colin.dang.pojo.CategoryProduct">
		<primary-key>
			<generator class="identity"></generator>
		</primary-key>
	</table>
	<table name="d_category" catalog="dangdang"
		class="org.colin.dang.pojo.Category">
		<primary-key>
			<generator class="identity"></generator>
		</primary-key>
	</table>
	<table name="d_item" catalog="dangdang"
		class="org.colin.dang.pojo.Item">
		<primary-key>
			<generator class="identity"></generator>
		</primary-key>
	</table>
	<table name="d_order" catalog="dangdang"
		class="org.colin.dang.pojo.Order">
		<primary-key>
			<generator class="identity"></generator>
		</primary-key>
	</table>
	<table name="d_product" catalog="dangdang"
		class="org.colin.dang.pojo.Product">
		<primary-key>
			<generator class="identity"></generator>
		</primary-key>
	</table>
	<table name="d_receive_address" catalog="dangdang"
		class="org.colin.dang.pojo.ReceiveAddress">
		<primary-key>
			<generator class="identity"></generator>
		</primary-key>
	</table>
<table name="d_user" catalog="dangdang"
	class="org.colin.dang.pojo.User">
	<primary-key>
		<generator class="identity"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>